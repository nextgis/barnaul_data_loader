#include "wxgis/resource.h"
#include "wxgis/version.h"
#include "version_dll.h"

#define APSTUDIO_HIDDEN_SYMBOLS
#include <windows.h>
#undef APSTUDIO_HIDDEN_SYMBOLS
#include <winver.h>

#if !defined(AFX_RESOURCE_DLL) || defined(AFX_TARG_NEU)
#ifdef _WIN32
LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
#pragma code_page(1252)
#endif //_WIN32

/////////////////////////////////////////////////////////////////////////////
//
// Version
//

#ifdef _DEBUG
    #define DLL_FLAGS 0x1L
#else
    #define DLL_FLAGS 0x0L
#endif

// 0x0409 is US English, 0x04b0 is Unicode and 0x0000 is 7 bit ASCII. see
// http://msdn.microsoft.com/en-us/library/aa381049(VS.85).aspx for the full
// list of languages and charsets
#define LANG 0x0409
#ifdef _UNICODE
    #define CHARSET 0x4b0
    #define LANG_WITH_CHARSET "040904b0"
#else
    #define CHARSET 0
    #define LANG_WITH_CHARSET "04090000"
#endif

VS_VERSION_INFO VERSIONINFO
 FILEVERSION        wxGISMON_MAJOR_VERSION,wxGISMON_MINOR_VERSION,wxGISMON_RELEASE_NUMBER,wxGISMON_SUBRELEASE_NUMBER
 PRODUCTVERSION     wxGISMON_MAJOR_VERSION,wxGISMON_MINOR_VERSION,wxGISMON_RELEASE_NUMBER,wxGISMON_SUBRELEASE_NUMBER
 FILEFLAGSMASK      0x3fL
 FILEFLAGS DLL_FLAGS
 FILEOS             0x4L
 FILETYPE           0x2L
 FILESUBTYPE        0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK LANG_WITH_CHARSET
        BEGIN
            VALUE "Comments", "Monitoring remote DB library from wxGIS project\0"
            VALUE "CompanyName", "wxGIS development team\0"
#ifdef _WIN64
            VALUE "FileDescription", "wxGISMonitoring(x64) for MSW\0"
#else
            VALUE "FileDescription", "wxGISMonitoring(x86) for MSW\0"
#endif
            VALUE "FileVersion", wxGISMON_VERSION_NUM_DOT_STRING "\0"
            VALUE "InternalName", wxGISMON_FILENAME "\0"
            VALUE "LegalCopyright", "Copyright 2005-2012 wxGIS development team\0"
            VALUE "LegalTrademarks", "\0"
            VALUE "OriginalFilename", wxGISMON_FILENAME ".dll\0"
            VALUE "PrivateBuild", "\0"
            VALUE "ProductName", "wxGISMonitoring\0"
            VALUE "ProductVersion", wxGISMON_VERSION_NUM_DOT_STRING "\0"
            VALUE "SpecialBuild", "\0"            
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", LANG, CHARSET
    END
END

#endif
